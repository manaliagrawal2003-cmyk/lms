 {
  "semi": true, 
  "singleQuote": true, 
  "tabWidth": 2, 
  "useTabs": false,
  "printWidth": 100, 
  "trailingComma": "es5",
  "bracketSpacing": true, 
  "arrowParens": "always", 
  "endOfLine": "auto", 
  "jsxSingleQuote": false, 
  "bracketSameLine": false, 
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrder": ["^react", "^@?\\w", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
